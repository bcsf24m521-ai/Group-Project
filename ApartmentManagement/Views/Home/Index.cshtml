<section id="hero" class="hero section">

    <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="hero-content">
            <div class="row align-items-center">

                <div class="col-lg-6 hero-text" data-aos="fade-right" data-aos-delay="200">
                    <div class="hero-badge">
                        <i class="bi bi-star-fill"></i>
                        <span>Premium Apartments</span>
                    </div>
                    <h1>Discover Your Perfect Home in the Heart of the City</h1>
                    <p>
                        Simplifying property management with reliable, efficient, and transparent services.
                        From tenant support to rent handling — we manage everything so you don’t have to.
                        Your properties, professionally maintained and always in safe hands.
                    </p>

                    <div class="search-form" data-aos="fade-up" data-aos-delay="300">
                        <form asp-controller="User" asp-action="BrowseApartments" method="get">
                            <div class="row g-3">
                                <div class="col-12">
                                    <div class="form-floating">
                                        <input type="text" class="form-control" id="location" name="searchString" placeholder="Location">
                                        <label for="location">Location or Keyword</label>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="form-floating">
                                        <select class="form-select" id="Apartment-type" name="type">
                                            <option value="">Select Type</option>
                                            <option value="Family">Family Apartment</option>
                                            <option value="CreativeStudio">Creative Studio</option>
                                            <option value="Studio">Studio Apartment</option>
                                            <option value="Office">Office</option>
                                            <option value="Penthouse">Penthouse</option>
                                        </select>
                                        <label for="Apartment-type">Apartment Type</label>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="form-floating">
                                        <select class="form-select" id="price-range">
                                            <option value="">Price Range</option>
                                            <option value="0-70000">Under Rs.70k</option>
                                            <option value="70000-150000">Rs.70k - Rs.105k</option>
                                            <option value="150000-250000">Rs.105k - Rs.250k</option>
                                            <option value="250000-500000">Rs.250k - Rs.500k</option>
                                            <option value="500000+">Above Rs.500k</option>
                                        </select>
                                        <label for="price-range">Price Range</label>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="form-floating">
                                        <select class="form-select" id="bedrooms">
                                            <option value="">Bedrooms</option>
                                            <option value="1">1 Bedroom</option>
                                            <option value="2">2 Bedrooms</option>
                                            <option value="3">3 Bedrooms</option>
                                            <option value="4">4 Bedrooms</option>
                                            <option value="5+">5+ Bedrooms</option>
                                        </select>
                                        <label for="bedrooms">Bedrooms</label>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="form-floating">
                                        <select class="form-select" id="bathrooms">
                                            <option value="">Bathrooms</option>
                                            <option value="1">1 Bathroom</option>
                                            <option value="2">2 Bathrooms</option>
                                            <option value="3">3 Bathrooms</option>
                                            <option value="4+">4+ Bathrooms</option>
                                        </select>
                                        <label for="bathrooms">Bathrooms</label>
                                    </div>
                                </div>

                                <div class="col-12">
                                    <button type="submit" class="btn btn-search w-100">
                                        <i class="bi bi-search"></i>
                                        Search Properties
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>

                    <div class="hero-stats" data-aos="fade-up" data-aos-delay="400">
                        <div class="row">
                            <div class="col-4">
                                <div class="stat-item">
                                    <h3><span data-purecounter-start="0" data-purecounter-end="2847" data-purecounter-duration="1" class="purecounter"></span>+</h3>
                                    <p>Apartments Listed</p>
                                </div>
                            </div>
                            <div class="col-4">
                                <div class="stat-item">
                                    <h3><span data-purecounter-start="0" data-purecounter-end="156" data-purecounter-duration="1" class="purecounter"></span>+</h3>
                                    <p>Verified Agents</p>
                                </div>
                            </div>
                            <div class="col-4">
                                <div class="stat-item">
                                    <h3><span data-purecounter-start="0" data-purecounter-end="98" data-purecounter-duration="1" class="purecounter"></span>%</h3>
                                    <p>Client Satisfaction</p>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="col-lg-6 hero-images" data-aos="fade-left" data-aos-delay="400">
                    <div class="image-stack">
                        <div class="main-image">
                            <img src="~/assets/img/real-estate/property-exterior-3.webp" alt="Luxury Property" class="img-fluid">
                            <div class="property-tag">
                                <span class="price">Rs.850,000</span>
                                <span class="type">Featured</span>
                            </div>
                        </div>
                        <div class="secondary-image">
                            <img src="~/assets/img/real-estate/property-interior-7.webp" alt="Property Interior" class="img-fluid">
                        </div>
                        <div class="floating-card">
                            <div class="agent-info">
                                <img src="~/assets/img/real-estate/agent-4.webp" alt="Agent" class="agent-avatar">
                                <div class="agent-details">
                                    <h5>Sarah Johnson</h5>
                                    <p>Top Real Estate Agent</p>
                                    <div class="rating">
                                        <i class="bi bi-star-fill"></i><i class="bi bi-star-fill"></i><i class="bi bi-star-fill"></i><i class="bi bi-star-fill"></i><i class="bi bi-star-fill"></i>
                                        <span>4.9 (127 reviews)</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>

    </div>

</section>

